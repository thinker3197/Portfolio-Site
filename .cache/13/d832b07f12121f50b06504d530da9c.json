ed;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\nbody {\n  height: 100vh;\n  padding: 24px;\n  font-family: \"Crimson Text\", serif;\n  color: $text-color;\n  font-size: 24px;\n  line-height: 33px;\n}\n\nmain {\n  height: 100%;\n  background-color: $section-bg;\n}\n\nsection {\n  height: 100%;\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\n.logo {\n  width: fit-content;\n  padding: 4px 8px;\n  background-color: $logo-bg;\n  border-bottom-right-radius: 4px;\n  border-bottom-left-radius: 4px;\n  height: 54px;\n  box-shadow: 2px 4px 4px $shadow-bg;\n}\n\n.container {\n  display: grid;\n  grid-template-columns: 50% 50%;\n  padding-top: 40px;\n}\n\n.about {\n  &__content {\n    h1 {\n      font-family: \"Playfair Display\", serif;\n      font-size: 100px;\n      line-height: 133px;\n      margin: 5px 0;\n    }\n  }\n\n  &__social {\n    & > ul {\n      @include flex(flex-start);\n      list-style: none;\n      padding: 0;\n    }\n  }\n\n  &__social-item {\n    @include flex;\n    width: 40px;\n    height: 40px;\n    margin-right: 20px;\n    border-radius: 50%;\n    padding: 8px;\n    filter: invert(8%) sepia(65%) saturate(2970%) hue-rotate(217deg)\n      brightness(81%) contrast(112%);\n\n    &:hover {\n      border: 1px solid $text-color;\n    }\n\n    & > img {\n      width: 100%;\n      height: auto;\n    }\n  }\n\n  &__resume {\n    margin-top: 35px;\n\n    & > button {\n      cursor: pointer;\n      background: none;\n      border: 2px solid $text-color;\n      border-radius: 25px;\n      padding: 10px 24px;\n    }\n  }\n}\n\n.profile {\n  position: relative;\n  justify-self: center;\n  padding-top: 80px;\n  padding-left: 40px;\n\n  &__img {\n    @include profile-img;\n    background-image: url(\"../img/profile-glitch.jpg\");\n    background-size: cover;\n    background-position: 70% 0;\n    position: relative;\n    z-index: 1;\n    transition: background-image 0.25s cubic-bezier(0.075, 0.82, 0.165, 1);\n\n