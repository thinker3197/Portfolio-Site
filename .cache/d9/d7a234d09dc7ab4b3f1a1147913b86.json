   &:hover {\n      border: 1px solid $text-color;\n    }\n\n    & > img {\n      width: 100%;\n      height: auto;\n    }\n  }\n\n  &__resume {\n    margin-top: 35px;\n\n    & > button {\n      cursor: pointer;\n      background: none;\n      border: 2px solid $text-color;\n      border-radius: 25px;\n      padding: 10px 24px;\n    }\n  }\n}\n\n.profile {\n  position: relative;\n  justify-self: center;\n  padding-top: 80px;\n  padding-left: 40px;\n\n  &__img {\n    @include profile-img;\n    background-image: url(\"../img/profile-glitch.jpg\");\n    background-size: cover;\n    background-position: 70% 0;\n    position: relative;\n    z-index: 1;\n    transition: background-image 0.5s ease-in-out;\n\n    &:hover {\n      background-image: url(\"../img/profile.jpg\");\n    }\n  }\n\n  &__backdrop {\n    @include profile-img;\n    background: $backdrop-bg;\n    position: absolute;\n    top: 113px;\n    left: 7px;\n    z-index: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"fd69a3f59f220ab3a217ea04c358c8c6","cacheData":{"env":{}}}